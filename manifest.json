{
    "manifest_version": 3,
    "name": "Bird Blocker",
    "description": "Base Level Extension",
    "permissions": ["activeTab"],
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "bird.png"
    },
    "content_scripts": [{
        "matches": ["*://twitter.com/*"],
        "js": ["blocker.js"]
    }]
}
